#canvas, #canvas2 {
  width: 400px;
  height: 400px;
}

#profile, #profile2 {
  display: none;
}

@font-face {
    font-family: 'condensed';
    src: url(<%= asset_path("condensed-webfont.eot")%>);
    src: url(<%= asset_path("condensed-webfont.eot?#iefix")%>) format('embedded-opentype'),
         url(<%= asset_path("condensed-webfont.woff")%>) format('woff'),
         url(<%= asset_path("condensed-webfont.ttf")%>) format('truetype'),
         url(<%= asset_path("condensed-webfont.svg#condensed")%>) format('svg');
    font-weight: normal;
    font-style: normal;
}

