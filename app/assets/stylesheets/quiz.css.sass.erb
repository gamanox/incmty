#help
  top: 50px
  right: 50%
  margin-right: -400px
  cursor: pointer
  height: 62px
  width: 62px
  display: block
  position: fixed
  z-index: 1500
  background-image: url('<%= asset_path("help.png")%>')
  background-position: 50% 50%
  background-repeat: no-repeat
  -webkit-transition: -webkit-transform 0.2s ease-in-out
  &:hover
    -webkit-transform: rotate(360deg)
  @media screen and (max-width: $break-small)
    display: none
    top: 10px
    right: 10px
    margin-right: 0
    background-size: cover
    width: 40px
    height: 40px


