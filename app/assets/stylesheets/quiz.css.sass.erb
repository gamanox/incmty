@-webkit-keyframes anifocus
  0%
    -webkit-filter: blur(0px)

  25%
    -webkit-filter: blur(3px)

  50%
    -webkit-filter: blur(0px)
  75%
    -webkit-filter: blur(3px)
  100%
    -webkit-filter: blur(0px)

$break-small: 850px
$break-large: 1200px

svg
  display: block

html, body, #screen
  width: 100%
  height: 100%
  margin: 0
  padding: 0
#screen
